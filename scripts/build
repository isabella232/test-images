#!/bin/bash

set -ex

if [ -x "$(which wrapdocker)" ]; then
    wrapdocker > /tmp/docker.log 2>&1
    docker ps
fi

cd $(dirname $0)

for dir in ../images/*; do
    NAME=$(basename $dir)
    TAG=$(<$dir/version)
    docker build -t ranchertest/$NAME:$TAG $dir
    echo Created ranchertest/$NAME:$TAG
done
